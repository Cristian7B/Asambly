{
    "version": 2,
    "builds": [
      {
        "src": "Asambly/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Asambly/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "Asambly.settings"
    }
  }
  